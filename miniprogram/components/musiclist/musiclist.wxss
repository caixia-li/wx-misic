.musiclist-container{
  display: flex;
  padding: 14rpx 20rpx;
  align-items: center;
  border-bottom: 1px solid #ddd;
}
.musiclist-index{
  color: #888;
  font-size: 34rpx;
  width: 80rpx;
}
.musiclist-info{
  flex-grow: 1;
  width: 0;
}
.musiclist-name{
  font-size: 34rpx;
  color: #333;
  overflow: hidden;
   text-overflow: ellipsis;
   white-space: nowrap;
   margin-bottom: 10rpx;
}
.musiclist-singer{
  color: #888;
  font-size: 24rpx;
}
.playing{
  color:#1296db;
}