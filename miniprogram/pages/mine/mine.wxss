.profile-header{
  text-align: center;
  width: 100%;
  padding: 90rpx 0rpx;
  background-color: #1296db;
}
.avatar-url{
  width: 250rpx;
  height: 250rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 0rpx auto 20rpx auto;
}
.nickName{
  color: #fff;
  font-size: 32rpx;
}
.nav{
  padding: 20rpx 0rpx;
}
.nav-item{
  width: 100%;
  height: 110rpx;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  padding: 0rpx 20rpx;
  border-bottom: 1px solid rgb(238, 233, 233);
}
.content{
  width: 100%;
}
.iconfont{
    color:#1196db;
    font-size: 32rpx;
    padding: 0rpx 20rpx;
}
.icon-xiangyou{
    color: #000000;
    font-size: 24rpx;
    float: right;
    margin-right: 20rpx;
}
